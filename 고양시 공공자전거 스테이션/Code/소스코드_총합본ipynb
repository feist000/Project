        "\n",
        "\n",
        "population_latest5[\"총인구수\"]= population_latest5[\"총인구수\"].astype(int)\n",
        "population_latest5[\"세대수\"]= population_latest5[\"세대수\"].str.replace(',','').astype(int)\n",
