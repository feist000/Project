        "# 대여가 많은 순으로 정류장 number를 뽑은 데이터\n",
        "b_leas "
      ],
      "execution_count": null,
      "outputs": [
        {
          "output_type": "execute_result",
          "data": {
            "text/html": [
              "<div>\n",
